<div class="jumbotron">
  <div class="container">
    <nav>
      <ul class="nav nav-tabs" id="nav-tab" role="tablist">
        <button class="nav-link active" id="nav-alpha-tab" data-bs-toggle="tab" data-bs-target="#nav-alpha" type="button" role="tab" aria-controls="nav-alpha" aria-selected="true">Alphabetical</button>
        <button class="nav-link" id="nav-techno-tab" data-bs-toggle="tab" data-bs-target="#nav-techno" type="button" role="tab" aria-controls="nav-techno" aria-selected="false">Techno</button>
        <button class="nav-link" id="nav-house-tab" data-bs-toggle="tab" data-bs-target="#nav-house" type="button" role="tab" aria-controls="nav-house" aria-selected="false">House</button>
        <button class="nav-link" id="nav-trance-tab" data-bs-toggle="tab" data-bs-target="#nav-trance" type="button" role="tab" aria-controls="nav-trance" aria-selected="false">Trance</button>

    </nav>
    <div class="tab-content" id="nav-tabContent">
      <div class="tab-pane fade show active" id="nav-alpha" role="tabpanel" aria-labelledby="nav-alpha-tab" tabindex="0">
        <ul>
        <% sortedAscFestivals.forEach(sortFestival => { %>
          <li><a href="/festival/<%-sortFestival.id%>"><button><%-sortFestival.name%></button></a>
              <img src=<%-sortFestival.imageUrl%> alt="Artwork"></li>
            <% }) %>
        </ul>
      </div>
      <div class="tab-pane fade" id="nav-techno" role="tabpanel" aria-labelledby="nav-techno-tab" tabindex="0">
        <ul>
          <% technoFestivals.forEach(technoFestival => { %>
            <li>
              <a href="/festival/<%-technoFestival._id%>"><button><%-technoFestival.name%></button></a>
              <img src=<%-technoFestival.imageUrl%> alt="Artwork">
            </li>
            <% }) %>
          </ul>
      </div>
      <div class="tab-pane fade" id="nav-house" role="tabpanel" aria-labelledby="nav-house-tab" tabindex="0">
        <ul>
          <% houseFestivals.forEach(houseFestival => { %>
            <li>
              <a href="/festival/<%-houseFestival._id%>"><button><%-houseFestival.name%></button></a>
              <img src=<%-houseFestival.imageUrl%> alt="Artwork">
            </li>
            <% }) %>
          </ul>
      </div>
      <div class="tab-pane fade" id="nav-trance" role="tabpanel" aria-labelledby="nav-trance-tab" tabindex="0">
        <ul>
          <% tranceFestivals.forEach(tranceFestival => { %>
            <li>
              <a href="/festival/<%-tranceFestival._id%>"><button><%-tranceFestival.name%></button></a>
              <img src=<%-tranceFestival.imageUrl%> alt="Artwork">
            </li>
            <% }) %>
        </ul>
      </div>
      </div>
    </div>
  </div>
</div>


